#include <stdio.h>
#include <math.h>
int main() {
double x,y,z, d, r1, r2, f1 ,f2;
printf("\nEnter x , y and z : ");
scanf("%lf %lf %lf" ,&x,&y,&z);
d = y * y - 4*x*z;
if(d>0){
r1 = (-y + sqrt(d)) / (2*x);
r2 = (-y - sqrt(d)) / (2*x);
printf("r1 = %.2lf and r2 = %.2lf" , r1,r2);
}
else if(d==0){
r1 = r2 = -y/(2*x);
printf("r1 = r2 = %.2lf" , r1);
}
else{
f1 = -y / (2*x);
f2 = sqrt(-d) / (2*x);
print("r1 = %.2lf+%.2lf and r2 = %.2lf-%.2lf"  , f1,f2,f1,f2);
}
return 0;
}
